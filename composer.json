{
    "name": "naept/laravel-indicator",
    "description": "A Laravel package to manage indicators on objects.",
    "type": "library",
    "authors": [
        {
            "name": "Naept",
            "email": "contact@naept.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/testbench" : "3.*",
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Naept\\LaravelIndicator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naept\\LaravelIndicator\\IndicatorServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev"
}
